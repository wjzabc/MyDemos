<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".TouchEventDispatchActivity" >

    <com.weibo.wjzabc.mydemos.views.MyViewGroup
        android:id="@+id/myViewGroup1"
        android:layout_width="600px"
        android:layout_height="600px"
        android:background="@android:color/black" >

        <com.weibo.wjzabc.mydemos.views.MyViewGroup2
            android:layout_width="500px"
            android:layout_height="500px"
            android:background="@android:color/white" >

            <com.weibo.wjzabc.mydemos.views.MyViewGroup3
                android:layout_width="400px"
                android:layout_height="400px"
                android:background="@android:color/black" >
                        <com.weibo.wjzabc.mydemos.views.MyView
            android:id="@+id/myView1"
            android:layout_width="300px"
            android:layout_height="300px"
            android:background="@android:color/white"
            android:text="@string/hello_world" />
            </com.weibo.wjzabc.mydemos.views.MyViewGroup3>
        </com.weibo.wjzabc.mydemos.views.MyViewGroup2>


    </com.weibo.wjzabc.mydemos.views.MyViewGroup>

</RelativeLayout>